// module:Alchs


// parameterize as desired
#define DROP(_name) if(name:_name) {color = BLACK; \
backgroundColor = GOLD; \
borderColor = BLACK; \
textAccent = 3; \
showLootbeam = false;\
}

DROP("* battlestaff")\
DROP("Dragon med helm")\
DROP("Lava battlestaff")\
DROP("Mystic air staff")\
DROP("Rune 2h sword")\
DROP("Rune battleaxe")\
DROP("Rune chainbody")\
DROP("Rune full helm")\
DROP("Rune halberd")\
DROP("Rune kiteshield")\
DROP("Rune med helm")\
DROP("Rune pickaxe")\
DROP("Rune platebody")\
DROP("Rune scimitar")\
DROP("Rune sq shield")\
DROP("Rune sword")\
DROP("Rune platelegs")\
DROP("Rune plateskirt")\


// endmodule:Alchs



